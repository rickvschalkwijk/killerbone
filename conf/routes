# HTTP methods: GET, POST, PUT, DELETE, HEAD

# Application
GET     /											controllers.Application.index()

# Authentication
POST	/authtoken									controllers.Authentication.createAuthToken()

# User Manager
GET		/user/:userId								controllers.UserManager.getUser(userId : Long)
POST	/user										controllers.UserManager.createUser()
PUT		/user/:userId								controllers.UserManager.updateUser(userId : Long)
DELETE	/user/:userId								controllers.UserManager.deleteUser(userId : Long)

# Friendship Manager
GET		/friendships/:userId						controllers.FriendshipManager.getUserFriendships(userId : Long)
POST	/friendship									controllers.FriendshipManager.createFriendship()
PUT		/friendship/:friendshipId/accept/:userId 	controllers.FriendshipManager.acceptFriendship(friendshipId : Long, userId : Long)
PUT		/friendship/:friendshipId/decline/:userId 	controllers.FriendshipManager.declineFriendship(friendshipId : Long, userId : Long)
PUT		/friendship/:friendshipId/end/:userId		controllers.FriendshipManager.endFriendship(friendshipId : Long, userId : Long)

# Development
GET		/development/allusers						controllers.Development.getAllUsers()
GET		/development/allfriendships					controllers.Development.getAllFriendships()